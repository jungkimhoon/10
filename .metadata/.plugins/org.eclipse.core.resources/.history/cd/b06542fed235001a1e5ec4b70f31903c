package thread;

public class singleTon {
	private int num=3;
	
	public static void main(String[] args) {
		SingleTon aa = new SingleTon();
		aa.num++;
		System.out.println("aa = "+aa);
		System.out.println("num = " + aa.num);
		System.out.println();
		
		SingleTon aa = new SingleTon();
		aa.num++;
		System.out.println("aa = "+aa);
		System.out.println("num = " + aa.num);
		System.out.println();
	}
	
}
